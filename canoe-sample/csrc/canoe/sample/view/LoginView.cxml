<components:Container xmlns:core="canoe.core" xmlns:eskin="canoe.sample.skin" xmlns:components="canoe.components" xmlns:skin="canoe.skin" xmlns:geom="flash.geom" xmlns:layout="canoe.layout" xmlns:filters="flash.filters" xmlns:text="flash.text" xmlns:view="canoe.sample.view">  <layout>    <layout:VLayout gap="10" hAlign="middle" vAlign="middle"/>  </layout>  <components:Container>    <layout>      <layout:HLayout gap="5"/>    </layout>    <core:Text text="用户名：" autoSize="left">      <style>        <core:TextStyle color="0xffff00"/>      </style>    </core:Text>    <components:TextInput id="tfPassport"/>  </components:Container>  <components:Container>    <layout>      <layout:HLayout gap="5"/>    </layout>    <core:Text text="密  码：" autoSize="left"/>    <components:TextInput id="tfPassword" displayAsPassword="@true"/>  </components:Container>  <components:Container>    <layout>      <layout:HLayout gap="10"/>    </layout>    <components:Button label="登录" id="btLogin"/>    <components:Button label="注册" id="btReg"/>  </components:Container></components:Container>