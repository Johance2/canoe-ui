<components:Skin xmlns:core="canoe.core" xmlns:components="canoe.components" xmlns:layout="canoe.layout" xmlns:state="canoe.state">  <states>    <state:State name="normal"/>    <state:State name="disabled"/>  </states>  <layout>    <layout:HLayout gap="2"/>  </layout>  <components:Button id="minimizeButton" label="|&lt;" width="20"/>  <components:Button id="decreaseButton" label="&lt;" width="20"/>  <core:Text text="第{data.value}页/共{data.maximum}页" autoSize="left"/>  <components:Button id="increaseButton" label=">" width="20"/>  <components:Button id="maximizeButton" label=">|" width="20"/>  <components:TextInput id="valueInput" width="30"/>  <components:Button id="confirmButton" width="40" label="确定"/></components:Skin>